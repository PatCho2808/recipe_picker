{
  "name": "recipe_picker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:node": "node src/server.js",
    "watch:node": "nodemon src/server.js",
    "debug": "node --inspect-brk server.js",
    "test": "mocha --reporter spec",
    "watch:sass": "sass --watch sass/main.scss public/css/style.css",
    "compile:sass": "sass --style=compressed sass/main.scss public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.comp.css -o public/css/style.css",
    "build:sass": "npm run compile:sass & npm run prefix:css",
    "minify:js": "uglifyjs public/js/*.js -o public/js/main.min.js",
    "dev": "npm run watch:node & npm run watch:sass",
    "build": "npm run build:sass & npm run minify:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PatCho2808/reciper_picker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PatCho2808/reciper_picker/issues"
  },
  "homepage": "https://github.com/PatCho2808/reciper_picker#readme",
  "dependencies": {
    "axios": "^0.20.0",
    "bootstrap": "^4.5.2",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.1",
    "jquery": "^3.5.1",
    "mongodb": "^3.6.0",
    "monk": "^7.3.2",
    "serve-favicon": "^2.5.0",
    "sass": "^1.32.8",
    "uglify-js": "^3.13.3",
    "postcss-cli": "^8.3.1",
    "autoprefixer": "^10.2.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "request": "^2.88.2"
  }
}
